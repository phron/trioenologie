<form method="post" action="{{ path('admin_picture_delete', {'id': picture.id}) }}" onsubmit="return confirm('Êtes-vous sûr.e de vouloir supprimer cette photo?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ picture.id) }}">    
    <div class="row justify-content-center mb-5">
        <button class="btn btn-delete col-lg-1 col-md-1 col-sm-2 mb-3">Supprimer l'image'</button>
    </div>
</form>
